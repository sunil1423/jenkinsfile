pipeline{
    agent any
    stages{
        stage('Building'){
            steps{
                sh 'mvn install'
                echo 'Building is done'
            }
        }
        stage('Rename'){
            steps{
                sh 'mv target/*.jar app.jar'
                echo 'rename is done'
            }
        }
        stage('execute'){
            steps{
                sh 'java -jar target/app.jar'
            }
        }
        stage('file'){
            steps{
                echo 'File is uploaded'
            }
        }
        
    }
}